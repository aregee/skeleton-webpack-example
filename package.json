{
  "name": "skeletonpwa",
  "version": "2.1.0",
  "description": "Lightweight vanilla javascript Progressive Webapp framework",
  "main": "bundle/index.js",
  "scripts": {
    "prod": "export NODE_SHELL_ENV=production && ./node_modules/webpack/bin/webpack.js --config webpack.config.js --progress ",
    "dev": "export NODE_SHELL_ENV=production && ./node_modules/webpack/bin/webpack.js --config webpack.config.js --progress -w",
    "serve": "webpack-dev-server --port 8080 --compress",
    "build-sw": "node sw-build.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vanilla",
    "pwa",
    "progressive",
    "web",
    "apps",
    "react",
    "angular",
    "vue",
    "mithril"
  ],
  "author": "Rahul Gaur",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.24.1",
    "bundle-loader": "^0.5.5",
    "copy-webpack-plugin": "^4.0.1",
