    "rxjs": "^5.4.2",
    "single-spa": "^3.5.0",
    "single-spa-angular2": "^1.1.0",
    "single-spa-react": "^2.0.4",
    "skeletonpwa": "../kernel",
    "svg-url-loader": "^2.2.0",
